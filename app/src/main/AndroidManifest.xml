<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.ufg.pes.healthhelp">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/health_help_icone_verde"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/HealthHelpTheme">

        <activity android:name="br.ufg.inf.pes.healthhelp.view.AgendaDisponivelActivity"></activity>
        <activity android:name="br.ufg.inf.pes.healthhelp.view.AgendaCompletaActivity"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity android:name="br.ufg.inf.pes.healthhelp.view.DetalhaAtendimentoActivity" >

        </activity>
        <activity
            android:name="br.ufg.inf.pes.healthhelp.view.HomeActivity"
            android:theme="@style/HealthHelpTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="br.ufg.inf.pes.healthhelp.view.LocalAtendimentoActivity" />

        <activity android:name="br.ufg.inf.pes.healthhelp.view.FormularioLocalAtendimentoActivity" />
        <activity android:name="br.ufg.inf.pes.healthhelp.view.AutenticacaoActivity" />
        <activity android:name="br.ufg.inf.pes.healthhelp.view.LocaisAtendimentoActivity" />
        <activity android:name="br.ufg.inf.pes.healthhelp.view.AtuacoesActivity" />

        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>
